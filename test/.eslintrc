{
    "extends": [
        "plugin:react/recommended",
    ],
    "rules": {
        "no-console": 0,
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off"
    },
    "env": {
        "jest": true,
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
}
